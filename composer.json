{
    "name": "darkwebdesign/doctrine-enhancement-bundle",
    "description": "Doctrine Enhancement Bundle",
    "type": "library",
    "keywords": ["bundle", "doctrine", "events"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {
            "name": "Raymond Schouten"
        }
    ],
    "require": {
        "php": ">=7.2",
        "darkwebdesign/doctrine-enhancement-pack": "2.9.*",
        "symfony/config": "^4.0 || ^5.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-symfony": "^1.4",
        "rector/rector": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhancementBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
